{
  "name": "my-money-backend",
  "version": "1.0.0",
  "description": "Backend API for My Money personal finance application",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "setup-env": "node scripts/setup-env.js",
    "prisma:migrate": "node scripts/run-with-db-url.js \"npx prisma migrate deploy\"",
    "prisma:generate": "node scripts/run-with-db-url.js \"npx prisma generate\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest tests/unit",
    "test:integration": "jest tests/integration",
    "test:ci": "jest --ci --coverage --watchAll=false"
  },
  "keywords": [
    "personal-finance",
    "api",
    "express",
    "postgresql"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@aws-sdk/client-secrets-manager": "^3.896.0",
    "@prisma/client": "^6.15.0",
    "bcrypt": "^6.0.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "express": "^5.1.0",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.16.3"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "jest": "^30.1.1",
    "jest-mock-extended": "^4.0.0",
    "nodemon": "^3.1.10",
    "prisma": "^6.15.0",
    "supertest": "^7.1.4"
  }
}
